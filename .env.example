# Model Router Agents - Environment Configuration Template
# Copy this file to .env and replace with your actual values

# Azure OpenAI Configuration for Simple Custom Router
AZURE_OPENAI_API_KEY=your-azure-openai-api-key-here
AZURE_OPENAI_ENDPOINT=https://your-resource-name.openai.azure.com
AZURE_OPENAI_API_VERSION=2024-02-15-preview
AZURE_OPENAI_CHAT_DEPLOYMENT_NAME=gpt-4o-mini

# Azure OpenAI Configuration for Pre-built Router
AZURE_OPENAI_API_KEY_ROUTER=your-router-api-key-here
AZURE_OPENAI_ENDPOINT_ROUTER=https://your-router-endpoint.openai.azure.com/
AZURE_OPENAI_API_VERSION_ROUTER=2024-12-01-preview

# Additional variables for agent framework compatibility
MODEL_ROUTER_AZURE_OPENAI_API_KEY=your-model-router-api-key-here
MODEL_ROUTERAZURE_OPENAI_ENDPOINT=https://your-model-router-endpoint.openai.azure.com/

# Optional: Logging and Debug Settings
# LOG_LEVEL=INFO
# DEBUG_MODE=false
